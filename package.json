{
  "name": "nodetypescript",
  "version": "1.0.0",
  "description": "Typescript Node.js API",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "start:dev": "ts-node-dev --transpile-only src/index.ts",
    "test:functional": "jest --projects ./test --runInBand",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test  --ext .ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pradoRebeca/node-typescript.git"
  },
  "author": "Rebeca Prado",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pradoRebeca/node-typescript/issues"
  },
  "homepage": "https://github.com/pradoRebeca/node-typescript#readme",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.1",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^17.0.36",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "3.2.0",
    "@typescript-eslint/parser": "3.2.0",
    "eslint": "7.2.0",
    "jest": "^28.1.0",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "body-parser": "^1.20.0",
    "express": "^4.18.1",
    "module-alias": "^2.2.2"
  }
}
